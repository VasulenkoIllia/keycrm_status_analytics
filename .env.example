# API key для запитів до keyCRM OpenAPI
KEYCRM_API_TOKEN=your_keycrm_api_key_here

# Базовий URL, за замовчуванням продакшн
KEYCRM_BASE_URL=https://openapi.keycrm.app/v1

# Login для фронту /api/*
AUTH_USER=admin
AUTH_PASS=admin123
# Секрет для підпису токенів
JWT_SECRET=change_me_jwt_secret
# Секрет для вебхука (x-webhook-token або ?token=)
WEBHOOK_TOKEN=change_me_webhook_secret

# Postgres
PGHOST=localhost
PGPORT=5432
PGUSER=keycrm
PGPASSWORD=keycrm
PGDATABASE=keycrm

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
